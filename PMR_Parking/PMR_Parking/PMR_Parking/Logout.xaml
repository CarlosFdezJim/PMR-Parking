<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PMR_Parking.Logout">
    
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand" Orientation="Vertical">
            <Label Text="¿Está seguro que desa salir?" x:Name="MyUserName" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand"/>
            <Button x:Name="Exit" Text="Logout" BackgroundColor="Black" TextColor="White" Clicked="Logout_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>